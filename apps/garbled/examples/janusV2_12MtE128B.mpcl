// -*- go -*-

package main

import (
	"crypto/cipher/cbc"
	"crypto/hmac"
)

// python console, to generate 2kB
// a = "01010101010101010101010101010101" // 16
// b = ""
// >>> for i in range(128): // 16*2=32, 16*4=64, 16*8=128, 16*16=256, 16*32=512, 16*64=1024, 16*128=2048
// ...     b += a
// ...
// >>> print(b)

// 16B: a = "0x01010101010101010101010101010101"
// 32B: a = "0x0101010101010101010101010101010101010101010101010101010101010101"
// 64B: a = "0x01010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101"
// 128B: a = "0x0101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101"
// 256B: a = ""
// 512B: a = ""
// 1024B: a = ""
// 2048B: a = ""

// MPCLDIR=./../../ ./garbled -v -i 0x72fff5b637aadfd02f22a1aea275c56705e94628bcd8a5ec2fa53914ff8d1250 examples/janusV2_12MtE128B.mpcl
type Garbler struct {
	cipherChunkResp [32]byte
}

// MPCLDIR=./../../ ./garbled -e -v -i 0x0101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101010101 examples/janusV2_12MtE128B.mpcl
type Evaluator struct {
	plainChunkResp [128]byte
}

func main(g Garbler, e Evaluator) uint8 { // uint8, ([]byte, []byte), []byte, [32]uint8

	// keys are added first
	key2 := []byte{
		0xc2, 0x86, 0x69, 0x6d, 0x88, 0x7c, 0x9a, 0xa0,
		0x61, 0x1b, 0xbb, 0x3e, 0x20, 0x25, 0xa4, 0x5a,
	}
	iv2 := []byte{
		0x56, 0x2e, 0x17, 0x99, 0x6d, 0x09, 0x3d, 0x28,
		0xdd, 0xb3, 0xba, 0x69, 0x5a, 0x2e, 0x6f, 0x58,
	}
	mac := hmac.SumSHA256(key2, e.plainChunkResp)

	// cbc of last blocks:
	cipher := cbc.EncryptAES128(key2, iv2, mac[:])

	// assert
	for i := 0; i < 32; i++ {
		if (cipher[i] ^ g.cipherChunkResp[i]) != 0x00 {
			return 0x00
		}
	}

	return 0x01
}
